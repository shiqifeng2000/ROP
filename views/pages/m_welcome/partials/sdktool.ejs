<md-list class="md-dense">
  <md-subheader class="md-no-sticky"><%= __('Welcome.sdktool.title')%></md-subheader>
  <md-list-item class="md-3-line md-long-text" ng-repeat="item in sdkcontent track by $index" ng-click="null">
    <img ng-if="item._title == '.net'" src="resource/dotnet-icon.jpg"/>
    <img ng-if="item._title == 'java'" src="resource/java-icon.jpg"/>
    <img ng-if="item._title == 'php'" src="resource/php-icon.jpg"/>
    <img ng-if="item._title == 'python'" src="resource/python-icon.jpg"/>
    <img ng-if="item._title == 'js'" src="resource/js-icon.jpg"/>
    <img ng-if="item._title == 'android'" src="resource/android-icon.jpg"/>
    <div class="md-list-item-text" layout="column">
      <h3>{{ item.name }}</h3>
      <h4>文件大小：{{ item.size }}</h4>
      <p>{{ item.time }}</p>
    </div>
  </md-list-item>

</md-list>
