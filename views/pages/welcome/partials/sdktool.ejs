<div class="sdk-list" flex-offset-gt-lg="20" flex-gt-lg="60" flex-offset-lg="15" flex-lg="70" flex-offset="10"
     flex="80">
  <div class="heado">
    <h2 class="md-title"><%= __('Welcome.sdktool.title') %></h2>
    <p class="md-subhead"><%= __('Welcome.sdktool.subtitle') %></p>
  </div>
  <h4 class="md-body-2 sepo"><%= __('Welcome.sdktool.pc') %></h4>
  <md-grid-list md-cols="8" md-row-height="2:2" md-gutter="24px">
    <md-grid-tile md-rowspan="2" md-colspan="2" ng-if="sdk['.net']">
      <div class="overlay-container">
        <div class="portrait">
          <img src="resource/dotnet-icon.jpg"/>
        </div>
        <table class="md-datatable">
          <tbody>
          <tr>
            <th><%= __('Welcome.sdktool.name') %></th>
            <td>{{sdk['.net'].name}}</td>
          </tr>
          <tr>
            <th><%= __('Welcome.sdktool.size') %></th>
            <td>{{sdk['.net'].size}}</td>
          </tr>
          <tr>
            <th><%= __('Welcome.sdktool.updatetime') %></th>
            <td>{{sdk['.net'].time}}</td>
          </tr>
          </tbody>
        </table>
        <div class="overlay-link">
          <p class="floating-text md-body-2">{{sdk['.net'].sdk_desc}}</p>
          <md-button class="md-raised md-primary floating-fab md-row-button" aria-label="Settings"
                     ng-click="locate(sdk['.net'].path)">
            <%= __('Welcome.sdktool.download') %>
          </md-button>
        </div>
      </div>
    </md-grid-tile>
    <md-grid-tile md-rowspan="2" md-colspan="2" ng-if="sdk['java']">
