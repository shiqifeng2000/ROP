<% include ./footer.ejs %>
<script>
  var type = 'anonymous';
  try {
    type = JSON.parse(getCookie("_session")).login_user_type
  } catch (e) {

  }
  bassdk.quick('setDefaultAttr');
  bassdk.track('visit', {
    pageName: 'SDK下载页',
    userType: type,
    pageType: "前端页面"
  });
</script>
